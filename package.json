    "@turf/turf": "^7.3.2",
    "@types/pg": "^8.16.0",
    "@types/three": "^0.182.0",
    "element-plus": "^2.12.0",
    "exifr": "^7.1.3",
    "maplibre-gl": "^5.15.0",
    "pg": "^8.18.0",

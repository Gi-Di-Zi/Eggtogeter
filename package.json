    "@types/google.maps": "^3.58.1",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "jimp": "^1.6.0",
    "supabase": "^2.75.3",

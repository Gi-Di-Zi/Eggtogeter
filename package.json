  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "vue-tsc -b --pretty false",
    "build": "npm run typecheck && vite build",
    "preview": "vite preview",
    "test": "npm run typecheck",
    "rpc:setup": "node scripts/setup_rpc.js",
    "rpc:grant": "node scripts/grant_rpc_perms.js",
    "rpc:cache:reload": "node scripts/fix_rpc_cache.js",
    "notion:sync": "python3 scripts/notion_sync_settings.py",
    "notion:watch": "python3 scripts/notion_sync_watch.py",
    "notion:bootstrap": "python3 scripts/notion_bootstrap_pull.py",
    "notion:bootstrap:apply": "python3 scripts/notion_bootstrap_apply.py",
    "wsl:doctor": "bash scripts/wsl_doctor.sh",
    "supabase:wsl": "npx supabase",
    "supabase:wsl:version": "npx supabase --version",
    "supabase:wsl:status": "npx supabase status -o json",
    "supabase:wsl:fallback": "bash scripts/supabase_cli_wsl.sh"
